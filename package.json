{
  "name": "nextjs-chakra-ui-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook",
    "build:export": "next build && next export",
    "dev": "next dev",
    "start": "next",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "npm test -- -u",
    "test:ci": "jest --ci",
    "storybook": "start-storybook -s ./public -p 9001",
    "lint": "eslint \"./**/*.{js,jsx}\" --fix",
    "format": "prettier --write \"./**/*.{js,jsx}\"",
    "generate": "npx plop --plopfile generators/plopfile.js"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.14",
    "@chakra-ui/react": "^1.6.5",
    "@chakra-ui/theme-tools": "1.1.8",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "framer-motion": "4.1.17",
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.2.2",
    "@babel/core": "^7.15.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/jest": "^11.3.0",
    "@storybook/addon-a11y": "^6.3.6",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-docs": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-storyshots": "^6.3.6",
    "@storybook/addon-viewport": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{js,css,json,md,mdx}": [
      "prettier --write",
      "git add"
    ]
  }
}
